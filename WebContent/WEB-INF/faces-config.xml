<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<application>
		<message-bundle>resources.application</message-bundle>
		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>
		<message-bundle>resources.messages</message-bundle>
		<locale-config>
			<default-locale>es</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
	</application>
	<managed-bean>
		<managed-bean-name>beanLogin</managed-bean-name>
		<managed-bean-class>beans.BeanLogin</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanRegistrar</managed-bean-name>
		<managed-bean-class>beans.BeanRegistrar</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanListarViaje</managed-bean-name>
		<managed-bean-class>beans.BeanListarViaje</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>beanRegistrarParada</managed-bean-name>
		<managed-bean-class>beans.BeanRegistrarParada</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>no</from-outcome>
			<to-view-id>/falloLogin.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>si</from-outcome>
			<to-view-id>/bienvenido.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/bienvenido.jsp</from-view-id>
		<navigation-case>
			<from-outcome>registrar</from-outcome>
			<to-view-id>/registrar.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/registrar.jsp</from-view-id>
		<navigation-case>
			<from-outcome>no</from-outcome>
			<to-view-id>/falloRegistrar.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/registrar.jsp</from-view-id>
		<navigation-case>
			<from-outcome>si</from-outcome>
			<to-view-id>/bienvenido.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/falloRegistrar.jsp</from-view-id>
		<navigation-case>
			<from-outcome>registrar</from-outcome>
			<to-view-id>/registrar.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/falloRegistrar.jsp</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/registrar.jsp</from-view-id>
		<navigation-case>
			<from-outcome>si</from-outcome>
			<to-view-id>/bienvenido.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/registrar.jsp</from-view-id>
		<navigation-case>
			<from-outcome>no</from-outcome>
			<to-view-id>/falloRegistrar.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
